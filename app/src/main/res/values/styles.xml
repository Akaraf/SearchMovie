        <item name="android:paddingBottom">@dimen/margin_8</item>
    </style>

    <style name="StartEndMargin16">
        <item name="android:layout_marginStart">@dimen/margin_16</item>
        <item name="android:layout_marginEnd">@dimen/margin_16</item>
    </style>

    <style name="StartEndMargin12">
        <item name="android:layout_marginStart">@dimen/margin_12</item>
        <item name="android:layout_marginEnd">@dimen/margin_12</item>
    </style>

    <style name="StartEndPadding16">
        <item name="android:paddingStart">@dimen/margin_16</item>
        <item name="android:paddingEnd">@dimen/margin_16</item>
    </style>

    <style name="TopBottomMargin16">
        <item name="android:layout_marginTop">@dimen/margin_16</item>
        <item name="android:layout_marginBottom">@dimen/margin_16</item>
    </style>

    <style name="TopBottomPadding16">
        <item name="android:paddingTop">@dimen/margin_16</item>
        <item name="android:paddingBottom">@dimen/margin_16</item>
    </style>

    <style name="AllMargin16">
        <item name="android:layout_marginStart">@dimen/margin_16</item>
        <item name="android:layout_marginTop">@dimen/margin_16</item>
        <item name="android:layout_marginEnd">@dimen/margin_16</item>
        <item name="android:layout_marginBottom">@dimen/margin_16</item>
    </style>

    <style name="AllMargin4">
        <item name="android:layout_marginStart">@dimen/margin_4</item>
        <item name="android:layout_marginTop">@dimen/margin_4</item>
        <item name="android:layout_marginEnd">@dimen/margin_4</item>
        <item name="android:layout_marginBottom">@dimen/margin_4</item>
    </style>

    <style name="AllMargin12">
        <item name="android:layout_marginStart">@dimen/margin_12</item>
        <item name="android:layout_marginTop">@dimen/margin_12</item>
        <item name="android:layout_marginEnd">@dimen/margin_12</item>
        <item name="android:layout_marginBottom">@dimen/margin_12</item>
    </style>

    <style name="HorizontalDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="background">?android:attr/listDivider</item>
    </style>

    <style name="VerticalDivider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="background">?android:attr/listDivider</item>
    </style>

    <style name="DayAppBarLayout">
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="NightAppBarLayout">
        <item name="colorPrimary">@color/black</item>
    </style>

    <style name="DaySearchView">
        <item name="queryBackground">@color/white</item>
    </style>

    <style name="NightSearchView">
        <item name="android:queryBackground">@color/black</item>
    </style>

    <style name="NightBottomNavigation">
        <item name="android:background">@color/black</item>
    </style>
