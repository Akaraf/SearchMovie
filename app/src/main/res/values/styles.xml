<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <style name="SearchMovie.Launcher" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowBackground">@drawable/background_splash</item>
        <item name="android:statusBarColor" tools:targetApi="l">@color/snow</item>
    </style>

    <style name="TextHead">
        <item name="android:layout_marginStart">@dimen/margin_16</item>
        <item name="android:layout_marginTop">@dimen/margin_32</item>
        <item name="android:layout_marginEnd">@dimen/margin_16</item>
        <item name="android:layout_marginBottom">@dimen/margin_16</item>
        <item name="android:textSize">@dimen/head_text_size</item>
    </style>

    <style name="TextD">
        <item name="android:layout_marginStart">@dimen/margin_16</item>
        <item name="android:layout_marginTop">@dimen/margin_24</item>
        <item name="android:layout_marginEnd">@dimen/margin_16</item>
        <item name="android:layout_marginBottom">@dimen/margin_16</item>
    </style>

    <style name="AllPadding16">
        <item name="android:paddingStart">@dimen/margin_16</item>
        <item name="android:paddingTop">@dimen/margin_16</item>
        <item name="android:paddingEnd">@dimen/margin_16</item>
        <item name="android:paddingBottom">@dimen/margin_16</item>
    </style>

    <style name="CategoryPadding">
        <item name="android:paddingStart">@dimen/margin_16</item>
        <item name="android:paddingTop">@dimen/margin_8</item>
